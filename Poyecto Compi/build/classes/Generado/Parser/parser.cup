/* Declaracion de terminales */
terminal Identificador,PalabraReservada, INT,FLOAT,CHAR,STRING,opAritmeticos,opComparadores,opAsignaciones,opDelimitadores,opBits,opLogicos,ERROR,END;

/* Declaracion de no terminales */
non terminal S, PROGRAMA, VARIABLE, FUNCION, DEFPARAMETROSINIT, DEFPARAMETROSVARIOS;

precedence left opDelimitadores;

/* inicio de gramatica */
start with PROGRAMA;

S::= PROGRAMA;
PROGRAMA::= FUNCION {: System.out.println("Identificador"); :}
| INT {: System.out.println("ebtero"); :};

VARIABLE::= PalabraReservada Identificador;

FUNCION::= PalabraReservada Identificador opDelimitadores DEFPARAMETROSINIT opDelimitadores;
DEFPARAMETROSINIT::= PalabraReservada Identificador DEFPARAMETROSVARIOS | ;
DEFPARAMETROSVARIOS::= opDelimitadores PalabraReservada Identificador DEFPARAMETROSVARIOS ;